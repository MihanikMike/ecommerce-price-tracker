{
  "name": "ecommerce-price-tracker",
  "version": "1.0.0",
  "type": "module",
  "description": "A Node.js application that monitors and tracks prices of products from e-commerce sites like Amazon and Burton, storing price history in PostgreSQL",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "migrate": "node src/cli/migrate.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "price-tracker",
    "web-scraping",
    "ecommerce",
    "playwright",
    "postgresql"
  ],
  "author": "MihanikMike",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "dotenv": "^16.4.5",
    "https-proxy-agent": "^7.0.6",
    "pg": "^8.16.3",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "playwright": "^1.57.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
